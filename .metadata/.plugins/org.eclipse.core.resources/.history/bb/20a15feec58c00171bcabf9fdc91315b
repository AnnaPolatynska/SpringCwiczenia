# connection string
spring.datasource.url=jdbc:mysql://localhost:3306/springtest?useSSL=false
#nazwa uzytkownika
spring.datasource.username=root2
#root password
spring.datasource.password=MySQL
#odswiezanie db przy kazdym uruchomieniu
spring.jpa.hibernate.ddl-auto=create
#zapytania widoczne w konsoli
spring.jpa.show-sql=true
